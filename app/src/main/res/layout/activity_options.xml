<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.gatecg.cs2340.rattitudem4.OptionsActivity">

    <LinearLayout
        android:layout_width="245dp"
        android:layout_height="121dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp">

        <Button
            android:id="@+id/rat_report_list_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="ratReportsListButton"
            android:text="Rat Reports List"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/return_to_options_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="optionsBackToWelcomeButton"
            android:text="Return To Welcome Page"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="71dp" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
