<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.gatecg.cs2340.rattitudem4.WelcomePageActivity">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <Button
            android:id="@+id/button2"
            android:layout_width="363dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.00"
            android:onClick="goBackBtn"
            android:text="Log Out"
            tools:layout_editor_absoluteX="148dp"
            tools:layout_editor_absoluteY="193dp"
            android:layout_marginTop="73dp"
            android:layout_below="@+id/rat_reports"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="360dp"
            android:layout_height="78dp"
            android:layout_weight="0.06"
            android:text="Welcome To Rattitude"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="30sp"
            tools:layout_editor_absoluteX="88dp"
            tools:layout_editor_absoluteY="115dp" />

        <Button
            android:id="@+id/rat_reports"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="52dp"
            android:onClick="ratReportsButton"
            android:text="Rat Reports" />
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
